    "f = open(\"new.txt\", \"r+\")\n",
    "f2 = open(\"old.txt\", \"w\")\n",
    "\n",
    "for line in f.readlines():\n",
    "    wordList = line.split('\t')\n",
    "    for eachWord in wordList:\n",
    "        f2.write(eachWord + \"\\n\")\n",
    "\n",
    "f.close()\n",
    "f2.close()"
